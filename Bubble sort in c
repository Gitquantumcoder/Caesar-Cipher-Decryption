#include <stdio.h>

int main(){
    int i,n,j;
    int arr[10];
    printf("enter 10 values in the array ");
    for(i=0;i<10;i++){
        scanf("%d",&arr[i]);
    }
    for(i=0;i<9;i++){
        for(j=0;j<9-i;j++){
            if(arr[j]>arr[j+1]){
                int c=arr[j];
                arr[j]=arr[j+1];
                arr[j+1]=c;}
        }
    }
    printf("new sorted array is;\n");
    for(i=0;i<10;i++){
        printf("%d\t",arr[i]);
    }
     return 0;
}
